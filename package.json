{
  "name": "chatbot",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "start": "vite --port 3000",
    "server": "node server.cjs",
    "server:build": "tsc server.ts --outDir . --module commonjs --target es2020 --moduleResolution node --esModuleInterop && mv server.js server.cjs",
    "server:dev": "npm run server:build && nodemon server.cjs",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx-source": "^7.27.1",
    "@stagewise-plugins/react": "^0.6.2",
    "@stagewise/toolbar-react": "^0.6.2",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-router": "^1.121.2",
    "@tanstack/react-router-devtools": "^1.121.2",
    "@tanstack/router-plugin": "^1.121.2",
    "click-to-react-component": "^1.1.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "lucide-react": "^0.525.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4.0.6"
  },
  "devDependencies": {
    "@locator/babel-jsx": "^0.4.4",
    "@locator/runtime": "^0.4.4",
    "@tanstack/eslint-config": "^0.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.0",
    "daisyui": "^5.0.47",
    "jsdom": "^26.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "web-vitals": "^4.2.4"
  }
}
